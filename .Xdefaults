! urxvt --help 2>&1| sed -n '/:  /s/^ */! URxvt*/gp'
! general settings
URxvt.title: rxvt-unicode-256color
URxvt*geometry: 80x40
URxvt*loginShell: True
URxvt*scrollTtyOutput:  False ! do not scroll to the bottom when tty receives output
URxvt*scrollWithBuffer: True
URxvt*jumpScroll: True
URxvt.termName: rxvt-unicode-256color
URxvt.imLocale: en_GB.UTF-8
URxvt.scrollBar: False
URxvt.scrollStyle: rxvt
URxvt.saveLines: 65535
URxvt.cursorBlink: True
URxvt.cursorUnderline: False
URxvt.borderLess: False
URxvt.transparency: False
URxvt.insecure: True
URxvt.iso14755: False
URxvt.iso14755_52: False
URxvt.visualBell: True
URxvt.urgentOnBell: True
URxvt*inheritPixmap: false
URxvt*dynamicColors: on
! Jump to bottom (prompt) on keypress.
URxvt*scrollTtyKeypress: true
URxvt*internalBorder: 0
URxvt*externalBorder: 0

! enable extensions
URxvt.perl-ext: default,matcher,tabbed,selection-to-clipboard

! clipboard
!! use in external programs
URxvt.clipboard.autocopy: True
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste

! URLs
URxvt.url-launcher: /usr/bin/firefox
! URxvt.matcher.button: 1
! URxvt.urlButton: 1
! URxvt.url-select.underline: True

! fonts
!URxvt.font: xft:Office Code Pro:size=10:antialias=true
!URxvt.boldFont: xft:Office Code Pro Bold:size=10:antialias=true
URxvt.font: xft:Input:pixelsize=14
URxvt.boldFont: xft:Input:pixelsize=14:bold
URxvt*lineSpace: 0
URxvt.letterSpace: 0
Xft.dpi: 96
Xft.rgba: rgb
Xft.antialias: 1
Xft.hinting: 1
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
! Xft.hintstyle: hintslight

! gruvbox theme
!! hard contrast: *background: #1d2021
!! norm contrast: *background: #282828
!! soft contrast: *background: #32302f
*background: #32302f
*foreground: #ebdbb2
!! darkGrey + black
*color8:  #928374
*color0:  #32302f
!! red + darkRed
*color9:  #fb4934
*color1:  #cc241d
!! green + darkGreen
*color10: #b8bb26
*color2:  #98971a
!! yellow + darkYellow
*color11: #fabd2f
*color3:  #d79921
!! blue + darkBlue
*color12: #83a598
*color4:  #458588
!! magenta + darkMagenta
*color13: #d3869b
*color5:  #b16286
!! cyan + darkCyan
*color14: #8ec07c
*color6:  #689d6a
!! white + lightGrey
*color15: #ebdbb2
*color7:  #a89984

! colors
URxvt*fading: 0
URxvt*colorUL: 4
URxvt.cursorColor:        #ebdbb2
URxvt*cursorColor2:       #282828
URxvt*highlightColor:     #ebdbb2
URxvt*highlightTextColor: #282828
! URxvt*troughColor: color
! URxvt*pointerColor: 5
! URxvt*pointerColor2: 6
! URxvt*borderColor: 6
! URxvt*underlineColor:  yellow
!! tabs
URxvt.tabbed.tabbar-fg: 236
URxvt.tabbed.tabbar-bg: 3
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 236
URxvt.tabbed.autohide: yes
URxvt.tabbed.new-button: no
